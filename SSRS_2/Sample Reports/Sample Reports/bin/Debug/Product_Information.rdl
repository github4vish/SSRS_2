  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>adv</DataSourceName>
        <CommandText>select p.name Maincategory,sub.Name Subcategory,convert(float,s.UnitPrice) as unitprice
,s.ModifiedDate from [Sales].[SalesOrderDetail] S
inner join [Production].[Product] P
on S.ProductID=p.ProductID

inner join 
[Production].[ProductSubcategory] Sub
on p.ProductSubcategoryID=sub.ProductSubcategoryID</CommandText>
      </Query>
      <Fields>
        <Field Name="Maincategory">
          <DataField>Maincategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
